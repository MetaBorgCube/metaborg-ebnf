module ebnf

imports
  
  Common

context-free start-symbols
  
  Start

context-free syntax
  
  Start.Grammar = <<{GrammarDef "\n"}*>>
  
  GrammarDef.Definition = <<Lhs> ::= <Rhs>>

  Lhs.Identifier = <<Identifier>>

  Rhs.Sequence = <<{Term " "}+>>

  Term.Identifier = <<Identifier>>
  Term.Terminal = <<Terminal>>

  Term.Choice = <<Rhs> | <Rhs>>
  Term.Opt = <[<Rhs>]>
  Term.Rep = <{<Rhs>}>
  Term.Grp = <(<Rhs>)>
